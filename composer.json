{
    "name": "restlessrancor/uptimerobotshield",
    "type": "phpbb-extension",
    "description": "Adds an uptime percentage in the footer using a shields.io image. A Monitor-Specific API key from https://uptimerobot.com is required.",
    "homepage": "https://www.restlessrancor.com",
    "version": "0.0.2-dev",
    "time": "2018-11-01",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Restless Rancor",
            "email": "contact@restlessrancor.com",
            "homepage": "https://www.restlessrancor.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "UptimeRobot Shield",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0,<3.3"
    },
    "version-check": {
            "host": "restlessrancor.com",
            "directory": "/extensions/versions",
            "filename": "uptimerobotshield.json",
            "ssl": "true"
            }
    }
}
